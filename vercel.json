{
  "version": 2,
  "name": "vidscribe2ai",
  "builds": [
    {
      "src": "api/submit.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/feedback.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/submit",
      "dest": "api/submit.js"
    },
    {
      "src": "/api/feedback",
      "dest": "api/feedback.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "github": {
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "buildCommand": "echo 'Building VidScribe2AI'"
}

