{
  "version": 2,
  "name": "vidscribe2ai",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "github": {
    "autoAlias": true,
    "autoJobCancelation": true
  }
}

